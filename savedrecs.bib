@article{ WOS:000806632900002,
Author = {Yang, Junwei and Zhang, Huiwen and Liu, Hui and Zhang, Jishi and Pei,
   Yong and Zang, Lihua},
Title = {Unraveling the roles of lanthanum-iron oxide nanoparticles in
   biohydrogen production},
Journal = {BIORESOURCE TECHNOLOGY},
Year = {2022},
Volume = {351},
Month = {MAY},
Abstract = {Low hydrogen (H-2) yield via dark fermentation often occurs, being
   mainly due to H-2 generation pathway shift. In this study,
   lanthanum-iron oxide nanoparticles (LaFeO3 NPs) were prepared to
   investigate their effects on bioH(2) production. The highest H-2 yield
   of 289.8 mL/g glucose was found at 100 mg/L of LaFeO3, being 47.6\%
   higher than that from the control (196.3 mL/g glucose). The relative
   abundance of Firmicutes increased from 54.2\% to 67.5\%. The large
   specific surface area of LaFeO3 provided sufficient sites for the
   colonization of Firmicutes and increased the bacterial access to
   nutrients. Additionally, the La3+ gradually released from LaFeO3 NPs
   raised microbial transmembrane transport capacity, promoting glycolytic
   efficiency and Fe availability, thereby increasing hydrogenase content,
   and shifting the bioH(2) evolution to butyrate pathway for more H-2.
   This provides the novelty for biochemical utilization of La and new
   insights into the improved H-2 yield amended with LaFeO3.},
